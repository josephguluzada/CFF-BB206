@model List<Student>;


<table border="1">
    <thead>
        <tr>
            <th>Id</th>
            <th>Fullname</th>
            <th>Age</th>
            <th>Grant</th>
            <th>GroupId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Student std in Model)
        {
            <tr>
                <td>@std.Id</td>
                <td>@std.Fullname</td>
                <td>@std.Age</td>
                <td>@std.Grant</td>
                <td>@std.GroupId</td>
            </tr>
        }
    </tbody>
</table>
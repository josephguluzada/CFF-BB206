@model HomeViewModel;
    <h1>Students:</h1>
<table border="1">
    <thead>
        <tr>
            <th>Id</th>
            <th>Fullname</th>
            <th>Age</th>
            <th>Grant</th>
            <th>GroupId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Student std in Model.Students)
        {
            <tr>
                <td>@std.Id</td>
                <td>@std.Fullname</td>
                <td>@std.Age</td>
                <td>@std.Grant</td>
                <td>@std.GroupId</td>
            </tr>
        }
    </tbody>
</table>

<h1>Teachers:</h1>
<table border="1">
    <thead>
        <tr>
            <th>Id</th>
            <th>Fullname</th>
            <th>Age</th>
            <th>Salary</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Teacher tch in Model.Teachers)
        {
            <tr>
                <td>@tch.Id</td>
                <td>@tch.Fullname</td>
                <td>@tch.Age</td>
                <td>@tch.Salary</td>
            </tr>
        }
    </tbody>
</table>